# Universal config for text editors
# https://EditorConfig.org

# top-most EditorConfig file
# Set to true to stop .editorconfig files search on current file.
root = true

[*]
insert_final_newline = true
charset = utf-8

[*.html]
indent_style = space
indent_size = 4
max_line_length = 140

[*.css]
indent_style = space
indent_size = 2
max_line_length = 140
